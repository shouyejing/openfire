<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        
        <record id="invoice_tree" model="ir.ui.view">
            <field name="name">account.invoice.tree</field>
            <field name="model">account.invoice</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="account.invoice_tree"/>
            <field name="arch" type="xml">
                <field name="name" position="replace"/>
                
                <field name="origin" position="replace">
                    <field name="name"/>
                    <field name="origin" invisible="1"/>
                </field>
            </field>
        </record>
        
        <!-- Ventes/Clients/Pistes Remplacement champs courriel et tÃ©l par CP et ville-->
        <record id="crm_lead_tree" model="ir.ui.view">
            <field name="name">crm.lead.tree</field>
            <field name="model">crm.lead</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="crm.crm_case_tree_view_leads"/>
            <field name="arch" type="xml">
                <field name="email_from" position="replace">
                    <field name="zip" string="CP"/>
                    <field name="city"/>
                </field>
            </field>
        </record>
        
    </data>
</openerp>